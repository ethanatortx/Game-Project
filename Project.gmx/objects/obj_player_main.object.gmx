        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// do player movement

var hspd = global.player_speed * global.moveModX;
var vspd = global.player_speed * global.moveModY;
var runAnimSpd = scrDeltaApply(.5,true);

global.axisDeadZone = 0.2;
var device = 0;
var gpinput = false;

var pad_num = gamepad_get_device_count();
var gp = undefined; // initialize var gp as a local variable for future use

for (i = 0; i &lt; pad_num; i++) {
    gp[i] = gamepad_is_connected(i); // get array of devices and whether or not they are gamepad inputs
}

if (gp[device]) { // if specified device slot is gamepad or not
    var gpinput = true;
} else {
    var gpinput = false;
}

if (gpinput) {
    // gamepad axis assignment
    gamepad_set_axis_deadzone(0,0.2)
    var vaxis = gamepad_axis_value(device, gp_axislv);
    var haxis = gamepad_axis_value(device, gp_axislh);
    
    scrMoveGamepad(hspd,vspd,runAnimSpd,haxis,vaxis);
    
} else if (!gpinput) {
    // keyboard axis assignment
    var rkey = keyboard_check(global.keybinds[0]);
    var lkey = keyboard_check(global.keybinds[1]);
    var ukey = keyboard_check(global.keybinds[2]);
    var dkey = keyboard_check(global.keybinds[3]);

    var keytemp = ds_list_create();
    ds_list_add(keytemp,rkey);
    ds_list_add(keytemp,lkey);
    ds_list_add(keytemp,ukey);
    ds_list_add(keytemp,dkey);
    
    scrMoveKeyboard(hspd,vspd,runAnimSpd,keytemp);
    
    ds_list_destroy(keytemp);
}
</string>
